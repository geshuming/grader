{
  "name": "grader",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "./scripts/build.sh",
    "typecheck": "tsc --noEmit",
    "test": "cross-env TIMEOUT=2000 jest --env=node --detectOpenHandles",
    "test-coveralls": "cross-env TIMEOUT=2000 jest --env=node --coverage --coverageReporters=text-lcov --forceExit | coveralls"
  },
  "dependencies": {
    "js-slang": "0.1.14",
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^10.14.8",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "jest": "^23.4.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/__tests__/test.*\\.ts$"
  }
}
